{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    ["babel-plugin-styled-components", {
      "ssr": true, 
      "displayName": true, 
      "preprocess": false 
    }],
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "components": "./components",
        "styles": "./styles",
      }
    }]
  ],
  "env": {
    "test": {
      "presets": [["next/babel", { "preset-env": { "modules": "commonjs" } }]]
    }
  }
}
